{
  "name": "newrelic",
  "version": "0.0.0",
  "private": true,
  "author": "The New Relic Browser Team",
  "description": "Tests for the New Relic JavaScript agent",
  "scripts": {
    "test": "node tools/jil/bin/cli.js",
    "build:all": "npx nx run-many --target=build && npm run packages:bundle && npm run cdn:build:local",
    "postinstall": "ln -s ../tools/jil node_modules/jil",
    "lint": "eslint --ext .js -c .eslintrc .",
    "lint:fix": "eslint --ext .js -c .eslintrc . --fix",
    "test-all": "tools/jil/bin/cli.js -b *@*",
    "ci-build": "npm ci --cache ./.npm",
    "build-tar": "npm run ci-build && tar czf ./build/browser-agent.tar.gz --exclude=.git --exclude=.npm --exclude=build/browser-agent.tar.gz .",
    "watch": "jung -D '\\.npm|.git|node_modules|build|tools|tests' --run -- npm run build:all",
    "cdn:build": "npm --prefix cdn run build",
    "cdn:build:extension": "npm --prefix cdn run build:extension",
    "cdn:build:local": "npm --prefix cdn run build:local",
    "cdn:build:prod": "npm --prefix cdn run build:prod",
    "cdn:build:dev": "npm --prefix cdn run build:dev",
    "cdn:build:current": "npm --prefix cdn run build:current",
    "cdn:build:nr": "npm --prefix cdn run build:nr",
    "cdn:build:pr": "npm --prefix cdn run build:pr",
    "cdn:watch": "jung -D '\\.npm|.git|node_modules|build|tools|tests|cdn' --run -- npm run cdn:build:local",
    "cdn:watch:extension": "jung -D '\\.npm|.git|node_modules|build|tools|tests|cdn' --run -- npm run cdn:build:extension",
    "packages:bundle": "webpack -c tools/jil/webpack.modular.js",
    "packages:build": "npm --prefix ./packages run build",
    "packages:pack": "npm --prefix packages/browser-agent run pack",
    "packages:test": "npm --prefix packages/browser-agent run test",
    "packages:test:watch": "npm --prefix packages/browser-agent run test:watch",
    "packages:watch": "jung -d './packages/browser-agent/src|./modules' --run -- npm run packages:build",
    "test-server": "node ./tools/jil/bin/server",
    "sauce:connect": "node ./tools/jil/bin/sauce",
    "sauce:get-browsers": "node ./tools/jil/util/sauce-browsers",
    "tools:test-builds": "npm --prefix ./tools/test-builds/build-time-mfe run build",
    "third-party-updates": "oss third-party manifest --includeOptDeps && oss third-party notices --includeOptDeps",
    "prepare": "husky install"
  },
  "config": {
    "unsafe-perm": true
  },
  "bin": {
    "jil": "./tools/jil/bin/cli.js",
    "jil-server": "./tools/jil/bin/server.js",
    "jil-sauce": "./tools/jil/bin/sauce.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newrelic/newrelic-browser-agent.git"
  },
  "dependencies": {
    "core-js": "^3.26.0",
    "lodash._slice": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@newrelic/newrelic-oss-cli": "^0.1.2",
    "@newrelic/nr-querypack": "https://git@github.com/newrelic/nr-querypack",
    "@nrwl/cli": "^15.0.9",
    "@nrwl/devkit": "^15.0.13",
    "@nrwl/eslint-plugin-nx": "^15.0.13",
    "@nrwl/jest": "15.0.10",
    "@nrwl/js": "^15.0.9",
    "@nrwl/workspace": "^15.0.9",
    "@types/jest": "28.1.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "aws-sdk": "^2.894.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babelify": "^10.0.0",
    "browserify": "^13.3.0",
    "bundle-collapser": "^1.2.1",
    "chalk": "^5.1.2",
    "charm": "^1.0.1",
    "concat-stream": "^1.5.1",
    "cors": "^2.7.1",
    "create-polyfill-service-url": "^2.2.6",
    "defined": "^1.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.29.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "function-bind": "^1.1.1",
    "glob": "^7.0.5",
    "husky": "^8.0.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jquery": "1.11.3",
    "jung": "^2.1.0",
    "just-debounce": "^1.0.0",
    "ls-stream": "^1.0.0",
    "mime-types": "^2.1.11",
    "minimatch": "^3.0.3",
    "multiparty": "^4.1.2",
    "newrelic": "^4.11.0",
    "node-fetch": "^3.2.10",
    "nx": "^15.0.9",
    "pre-commit": "^1.2.2",
    "object-inspect": "^1.5.0",
    "preprocessify": "0.0.6",
    "prettier": "^2.8.1",
    "request": "2.74.0",
    "sauce-connect-launcher": "^1.2.4",
    "semver": "^5.3.0",
    "server-destroy": "^1.0.1",
    "sinon": "^1.17.5",
    "tap-parser": "^1.2.2",
    "tape": "^4.8.0",
    "through": "^2.3.8",
    "through2": "^2.0.1",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "uglify-js": "^3.15.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.7.0",
    "wd": "^1.11.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^17.6.2"
  },
  "browser": {
    "jil": "./tools/jil/driver/browser.js"
  }
}

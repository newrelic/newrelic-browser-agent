      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./src/*": {
      "types": "./dist/types/*",
      "import": "./src/*",
      "default": "./src/*"
    },
    "./loaders/agent": {
      "types": "./dist/types/loaders/agent.d.ts",
      "require": "./dist/cjs/loaders/agent.js",
